{
  "name": "sequlize-boilerplate",
  "version": "1.0.0",
  "description": "A NodeJS+Sequelize boilerplate",
  "main": "index.js",
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "bcrypt": "^3.0.6",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "http-status-codes": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "redis": "^2.8.0",
    "sequelize": "^5.18.4",
    "supertest": "^4.0.2",
    "uniqid": "^5.0.3"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "sequelize-cli": "^5.5.1"
  },
  "scripts": {
    "production": "NODE_ENVIRONMENT=production node src/server.js",
    "development": "NODE_ENVIRONMENT=development nodemon src/server.js --ignore tests",
    "tests": "NODE_ENVIRONMENT=test jest",
    "pretests": "sequelize db:migrate --env test",
    "posttests": "sequelize db:migrate:undo --env test"
